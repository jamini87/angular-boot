{
  "name": "angular-boot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version:animation": " cd \"$(pwd)\"/projects/angular-boot/animation; npm version",
    "build:animation": "ng-packagr -p ./projects/angular-boot/animation/ng-package.json",
    "pack:animation": "npm pack ./dist/angular-boot/animation/",
    "publish:animation": "npm publish --access=public ./dist/angular-boot/animation/",
    "version:nb-assets": " cd \"$(pwd)\"/projects/angular-boot/nb-assets; npm version",
    "build:nb-assets": "ng-packagr -p ./projects/angular-boot/nb-assets/ng-package.json; cp -r ./projects/angular-boot/nb-assets/src/dist ./dist/angular-boot/nb-assets",
    "pack:nb-assets": "npm pack ./dist/angular-boot/nb-assets/",
    "publish:nb-assets": "npm publish --access=public ./dist/angular-boot/nb-assets/",
    "version:validation": " cd \"$(pwd)\"/projects/angular-boot/validation; npm version",
    "build:validation": "ng-packagr -p ./projects/angular-boot/validation/ng-package.json",
    "pack:validation": "npm pack ./dist/angular-boot/validation/",
    "publish:validation": "npm publish --access=public ./dist/angular-boot/validation/",
    "version:widgets": " cd \"$(pwd)\"/projects/angular-boot/widgets; npm version",
    "build:widgets": "ng-packagr -p ./projects/angular-boot/widgets/ng-package.json",
    "pack:widgets": "npm pack ./dist/angular-boot/widgets/",
    "publish:widgets": "npm publish --access=public ./dist/angular-boot/widgets/",
    "version:core": " cd \"$(pwd)\"/projects/angular-boot/core; npm version",
    "build:core": "ng-packagr -p ./projects/angular-boot/core/ng-package.json",
    "pack:core": "npm pack ./dist/angular-boot/core/",
    "publish:core": "npm publish --access=public ./dist/angular-boot/core/",
    "version:common": " cd \"$(pwd)\"/projects/angular-boot/common; npm version",
    "build:common": "ng-packagr -p ./projects/angular-boot/common/ng-package.json",
    "pack:common": "npm pack ./dist/angular-boot/common/",
    "publish:common": "npm publish --access=public ./dist/angular-boot/common/",
    "version:util": " cd \"$(pwd)\"/projects/angular-boot/util; npm version",
    "build:util": "ng-packagr -p ./projects/angular-boot/util/ng-package.json",
    "pack:util": "npm pack ./dist/angular-boot/util/",
    "publish:util": "npm publish --access=public ./dist/angular-boot/util/",
    "build:libs": "ng-packagr -p ./projects/angular-boot/animation/ng-package.json; ng-packagr -p ./projects/angular-boot/nb-assets/ng-package.json; cp -r ./projects/angular-boot/nb-assets/src/dist ./dist/angular-boot/nb-assets; ng-packagr -p ./projects/angular-boot/widgets/ng-package.json; ng-packagr -p ./projects/angular-boot/core/ng-package.json; ng-packagr -p ./projects/angular-boot/common/ng-package.json; ng-packagr -p ./projects/angular-boot/util/ng-package.json; ng-packagr -p ./projects/angular-boot/validation/ng-package.json;",
    "pack:libs": "npm pack ./dist/angular-boot/animation/; npm pack ./dist/angular-boot/nb-assets/; npm pack ./dist/angular-boot/widgets/; npm pack ./dist/angular-boot/core/; npm pack ./dist/angular-boot/common/; npm pack ./dist/angular-boot/util/; npm pack ./dist/angular-boot/validation/;",
    "publish:libs": "npm publish --access=public ./dist/angular-boot/animation/; npm publish --access=public ./dist/angular-boot/nb-assets/; npm publish --access=public ./dist/angular-boot/widgets/; npm publish --access=public ./dist/angular-boot/core/; npm publish --access=public ./dist/angular-boot/common/; npm publish --access=public ./dist/angular-boot/util/; npm publish --access=public ./dist/angular-boot/validation/;"
  },
  "private": true,
  "dependencies": {
    "@angular-boot/animation": "^0.3.24",
    "@angular-boot/common": "^0.3.24",
    "@angular-boot/core": "^0.3.24",
    "@angular-boot/nb-assets": "^0.3.24",
    "@angular-boot/util": "^0.3.24",
    "@angular-boot/validation": "^0.3.24",
    "@angular-boot/widgets": "^0.3.24",
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ng-select/ng-select": "^2.20.5",
    "@ngx-translate/core": "^11.0.1",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jalali-moment": "^3.3.3",
    "jquery": "^3.4.1",
    "ngx-scrollbar": "^4.2.0",
    "rxjs": "~6.3.3",
    "sweetalert2": "^7.33.1",
    "tslib": "^1.9.0",
    "xml-formatter": "^1.0.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
